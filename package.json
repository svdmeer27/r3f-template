{
  "name": "usersphere",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "cp -r ./public/ ./dist && yarn parcel ./src/index.html",
    "clear": "rm -rf ./dist ./.parcel-cache",
    "build": "rm -rf build/ && parcel build --dist-dir build src/index.html --no-source-maps && cp -r ./public ./build"
  },
  "dependencies": {
    "@react-three/fiber": "^8.17.7",
    "parcel": "^2.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.168.0"
  },
  "devDependencies": {
    "process": "^0.11.10"
  }
}
